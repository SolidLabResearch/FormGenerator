{
  "name": "form-generator",
  "version": "0.0.0",
  "private": true,
  "description": "A form generator app with Solid - Google Forms but the Solid way",
  "repository": "https://github.com/SolidLabResearch/FormGenerator",
  "license": "MIT",
  "author": "Ieben Smessaert",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@comunica/query-sparql": "^2.10.1",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@fortawesome/ember-fontawesome": "^0.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@smessie/solid-client-authn-browser": "^1.17.5",
    "bootstrap": "^5.2.0",
    "broccoli-asset-rev": "^3.0.0",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.6.3",
    "ember-bootstrap": "^5.1.1",
    "ember-cli": "~5.4.1",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-drag-drop": "^0.9.0-beta.0",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^8.0.0",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.4.0",
    "ember-template-lint": "^5.11.2",
    "ember-welcome-page": "^7.0.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "eyereasoner": "^11.1.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.0.3",
    "qunit": "^2.20.0",
    "qunit-dom": "^2.0.0",
    "rdflib": "^2.2.20",
    "solid-storage-root": "^1.0.1",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "tracked-built-ins": "^3.3.0",
    "uuid": "^8.3.2",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
